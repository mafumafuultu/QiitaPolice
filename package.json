    "less": "^3.11.3",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "electron-packager": "^15.0.0"
  },
  "scripts": {
    "start": "electron .",
    "less": "lessc -ru ./assets/less/main.less ./assets/css/main.css",
